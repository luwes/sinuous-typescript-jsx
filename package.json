{
  "name": "sinuous-jsx-demo",
  "version": "0.0.1",
  "description": "Sinuous JSX in TypeScript Demo Project",
  "main": "dist/app.js",
  "scripts": {
    "dev": "rollup -w -c",
    "build": "rollup -c",
    "start": "http-server -c-1 dist & yarn dev",
    "deploy": "yarn build && git add dist && (git diff-index --quiet HEAD || git commit -m 'Deploy') && git subtree push --prefix dist origin gh-pages"
  },
  "repository": "luwes/sinuous-typescript-jsx",
  "keywords": [
    "jsx",
    "tsx",
    "typescript",
    "ts",
    "sinuous"
  ],
  "author": "Ryan Cavanaugh (Microsoft)",
  "contributors": [
    "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)"
  ],
  "license": "Apache",
  "devDependencies": {
    "@rollup/plugin-typescript": "^2.0.1",
    "http-server": "^0.12.0",
    "rollup": "^1.27.14",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size": "^0.2.1",
    "rollup-plugin-terser": "^5.1.3",
    "tslib": "^1.10.0",
    "typescript": "latest"
  },
  "dependencies": {
    "sinuous": "^0.18.5"
  }
}
